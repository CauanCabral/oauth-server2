{
    "name": "cauancabral/oauth-server2",
    "description": "OauthServer2 plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "league/oauth2-server": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "OauthServer2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OauthServer2\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
